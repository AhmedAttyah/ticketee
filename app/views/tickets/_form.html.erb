<%= form_for [@project,@ticket] do |f| %>
	<% @ticket.errors.full_messages.each do |error| %>
		<h2><%= error %></h2>
	<% end %>
	<p>
	<%= f.label :title %>
	<%= f.text_field :title %>
	</p>
	<p>
	<%= f.label :description %> <br>
	<%= f.text_area :description %>
	</p>
	<p>
	<%= f.submit %>
	</p>

<% end %>